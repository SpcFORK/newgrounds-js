{
  "name": "newgrounds.js",
  "version": "4.0.0",
  "description": "The definitive Newgrounds.io library for Node.js and the browser.",
  "keywords": [
    "newgrounds",
    "newgrounds.io",
    "phaserjs",
    "phaser",
    "kaboom.js",
    "kaboom"
  ],
  "author": "lajbel <lajbelms@gmail.com>",
  "license": "MIT",

  "type": "module",
  "main": "./dist/newgrounds.cjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://newgroundsjs.lajbel.com/",
  "repository": "github:lajbel/newgrounds-js",
  "browser": "./dist/newgrounds.js",
  "module": "./dist/newgrounds.mjs",
  "scripts": {
    "start": "npm run build",
    "build": "node --watch-path ./src scripts/build.js",
    "dts": "npm-dts --force --entry newgrounds.js --output dist/index.d.ts -L debug generate"
  },
  "files": [
    "dist/",
    "source/"
  ],
  "exports": {
    "import": "./dist/newgrounds.mjs",
    "require": "./dist/newgrounds.cjs"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.12.7",
    "dts-generator": "^3.0.0",
    "esbuild": "^0.13.7",
    "typescipt": "^1.0.0"
  },
  "dependencies": {
    "@originjs/crypto-js-wasm": "^1.1.0",
    "crypto-js": "^4.2.0",
    "dprint": "^0.45.1"
  }
}
